import java.util.*;
    
    public class Main{
    
    public static void main(String[] args) {
      // write your code here  
      Scanner scn = new Scanner(System.in);
      int n = scn.nextInt();
      
      int temp = n;
      int count = 0;
      
      while(temp > 0){
          temp = temp/10;
          count++;
      }
      
      int power = (int)Math.pow(10,count-1);
      
      while(power>0){
          int data = n/power;
          n = n%power;
          System.out.println(data);
          power = power/10;
          
      }
      
      
     }
    }
